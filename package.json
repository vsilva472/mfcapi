    "morgan": "~1.9.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^4.7.0",
    "npm": "^6.9.0",
    "sequelize": "^5.3.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
